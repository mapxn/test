var $j = jQuery.noConflict();

$j(document).ready(function() {

    init_itoggle('app_8',change_upd2pro_enable);
    init_itoggle('app_9',change_upd2pro2_enable);
    init_itoggle('app_18',change_upd2pro3_enable);

});

