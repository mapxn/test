#!/bin/bash
#copyright by hiboy
source /etc/storage/script/init.sh

link="Advanced_Extensions_display.asp"
echo $1
if [ "$1"x = "stop"x ] ; then
    nvram set display_enable=0
    exit
fi

eval 'nvram set tablink'$1'='$link';'

nvram set display_L2="$(($3 + 10))"
nvram set display_L3="$2"
#     show_menu(8,<% nvram_get_x("", "display_L2"); %>,<% nvram_get_x("", "display_L3"); %>);

